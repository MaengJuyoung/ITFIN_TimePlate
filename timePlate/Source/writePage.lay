<div data-base="AView" data-class="writePage" class="AView-Style" style="left: 0px; top: 0px; width: auto; height: auto; background-color: rgba(0, 0, 0, 0); position: relative;" data-class-map="{&quot;afc&quot;:[&quot;AView&quot;,&quot;AButton&quot;,&quot;ALabel&quot;,&quot;ATextField&quot;,&quot;ATextArea&quot;]}">
    <div data-base="AView" data-class="AView" class="AView-Style" style="width: 600px; height: 700px; position: relative; left: 0px; top: 0px; background-color: rgb(255, 255, 255); border-width: 1px; border-style: solid; border-color: rgba(50, 110, 255, 0.8); border-radius: 5px; overflow: hidden;">
        <div data-base="AView" data-class="AView" class="AView-Style" style="width: 100%; height: 30px; position: relative; left: 0px; top: 0px; background-color: rgba(0, 0, 0, 0); border-bottom: 1px solid rgba(50, 110, 255, 0.5); margin-left: 0px; margin-top: 0px; padding: 5px;" id="writePage--top">
            <button data-base="AButton" data-class="AButton" class="AButton-Style" style="width: 20px; height: 20px; position: relative; left: 0px; top: 0px; float: right; font-size: 15px; font-weight: bold; font-family: 맑은고딕; color: rgb(50, 110, 255); background-color: rgba(0, 0, 0, 0);" data-listener-click="writePage:onCloseBtnClick">X</button>
        </div>
        <div data-base="AView" data-class="AView" class="AView-Style" style="width: 100%; height: 60px; position: relative; left: 0px; top: 0px; background-color: rgb(255, 255, 255); border-bottom: 1px solid rgba(50, 110, 255, 0.5); padding: 10px 20px; margin-top: 0px; display: flex; align-items: center;" id="writePage--writer_area">
            <label data-base="ALabel" data-class="ALabel" class="ALabel-Style" style="width: auto; height: auto; position: relative; left: 0px; top: 0px; font-size: 15px; margin-right: 10px; color: rgb(50, 110, 255); font-weight: bold;">작성자 :</label>
            <input data-base="ATextField" data-class="ATextField" type="text" value="" class="ATextField-Style" autocomplete="off" style="width: 150px; height: 30px; position: relative; left: 0px; top: 0px; background-color: rgb(255, 255, 255); border-width: 1px; border-style: solid; border-color: rgba(50, 110, 255, 0.5); border-radius: 5px;" id="writePage--writer">
            <label data-base="ALabel" data-class="ALabel" class="ALabel-Style" style="width: auto; height: auto; position: relative; left: 0px; top: 0px; font-size: 15px; margin-right: 10px; color: rgb(255, 0, 0); font-weight: bold; margin-left: 10px;" id="writePage--alert"></label>
        </div>
        <div data-base="AView" data-class="AView" class="AView-Style" style="width: 100%; height: 60px; position: relative; left: 0px; top: 0px; background-color: rgba(0, 0, 0, 0); border-bottom: 1px solid rgba(50, 110, 255, 0.5); padding: 10px 20px; margin-top: 0px; display: flex; align-items: center;" id="writePage--title_area">
            <label data-base="ALabel" data-class="ALabel" class="ALabel-Style" style="width: auto; height: auto; position: relative; left: 0px; top: 0px; font-size: 15px; margin-right: 10px; color: rgb(50, 110, 255); font-weight: bold;">제목 :</label>
            <input data-base="ATextField" data-class="ATextField" type="text" value="" class="ATextField-Style" autocomplete="off" style="width: 165px; height: 30px; position: relative; left: 0px; top: 0px; background-color: rgb(255, 255, 255); border-width: 1px; border-style: solid; border-color: rgba(50, 110, 255, 0.5); border-radius: 5px;" id="writePage--title">
        </div>
        <div data-base="AView" data-class="AView" class="AView-Style" style="width: 100%; height: 480px; position: relative; left: 0px; top: 0px; margin-left: 0px; margin-top: 0px; background-color: rgba(50, 110, 255, 0.2); border-bottom: 0px solid rgba(50, 110, 255, 0.8); padding: 10px 20px;" id="writePage--content_area">
            <label data-base="ALabel" data-class="ALabel" class="ALabel-Style" style="width: 100%; height: 40px; position: relative; left: 0px; top: 0px; font-size: 15px; margin-right: 10px; line-height: 40px; color: rgb(50, 110, 255); font-weight: bold;">내용 :</label>
            <textarea data-base="ATextArea" data-class="ATextArea" class="ATextArea-Style" style="width: 100%; height: 410px; position: relative; left: 0px; top: 0px; margin-left: 0px; margin-top: 0px; background-color: rgb(255, 255, 255); border-radius: 5px; border-width: 1px; border-style: solid; border-color: rgba(50, 110, 255, 0.5);" id="writePage--content"></textarea>
        </div>
        <div data-base="AView" data-class="AView" class="AView-Style" style="width: 100%; height: 70px; position: relative; left: 0px; top: 0px; background-color: rgba(0, 0, 0, 0); padding: 10px 190px; display: flex; justify-content: space-between; align-items: center; border-top: 1px solid rgba(50, 110, 255, 0.5);" id="writePage--buttom">
            <button data-base="AButton" data-class="AButton" class="AButton-Style" style="width: 100px; height: 40px; position: relative; left: 0px; top: 0px; border-width: 1px; border-style: solid; border-color: rgba(50, 110, 255, 0.5); border-radius: 5px; color: rgb(255, 255, 255); font-size: 15px; font-weight: bold; background-color: rgb(50, 110, 255);" data-listener-click="writePage:onSubmitBtnClick">확인</button>
            <button data-base="AButton" data-class="AButton" class="AButton-Style" style="width: 100px; height: 40px; position: relative; left: 0px; top: 0px; border-width: 1px; border-style: solid; border-color: rgba(50, 110, 255, 0.5); border-radius: 5px; color: rgb(255, 255, 255); font-size: 15px; font-weight: bold; background-color: rgb(50, 110, 255);" data-listener-click="writePage:onCloseBtnClick">취소</button>
        </div>
    </div>
</div>